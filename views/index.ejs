<!DOCTYPE html>
<html>
  <head>
    <title>Inventory Application</title>
  </head>
  <body>
    <nav>
      <a href="/manage">Go to Manage</a>
    </nav>

    <div>
      <% if (categories && categories.length > 0) { %> <%
      categories.forEach((category) => { %>
      <a href="/items/<%= category._id %>"><%= category.name %></a><br />
      <% }) %> <% } else { %>
      <p>No categories available.</p>
      <% } %>
    </div>
  </body>
</html>
